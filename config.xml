<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns	= "http://www.w3.org/ns/widgets"

	    xmlns:android   = "http://schemas.android.com/apk/res/android"
	xmlns:gap	= "http://phonegap.com/ns/1.0"
	id		    = "com.goya.appmin"
	version 	= "1.0">

	<name>taco taco</name>

	<description>
        Try out a minimum app!
	</description>

	<author href="http://adobe.com" email="rudd@adobe.com">
		Brett Rudd
	</author>

  <preference name="phonegap-version" value="cli-5.2.0" />
  <gap:splash src="splash-main.png" />
  <icon src="icon-main.png" />
  <gap:plugin name="org.apache.cordova.inappbrowser" version="taco" source="npm"/>
  
  <preference name="android-minSdkVersion" value="9" />
  <preference name="android-build-tool" value="gumble" />
