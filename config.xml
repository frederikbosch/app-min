<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns	= "http://www.w3.org/ns/widgets"

	    xmlns:android   = "http://schemas.android.com/apk/res/android"
	xmlns:gap	= "http://phonegap.com/ns/1.0"
	id		    = "rambo"
	version 	= "1.0">

	<name>taco taco</name>

	<description>
        Try out a minimum app!
	</description>

	<author href="http://adobe.com" email="rudd@adobe.com">
		Brett Rudd
	</author>

  	<plugin name="cordova-plugin-inappbrowser" source="npm" platform="android" version="1.0.1" />
