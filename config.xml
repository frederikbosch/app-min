<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns	        	= "http://www.w3.org/ns/widgets"
	xmlns:android   	= "http://schemas.android.com/apk/res/android"
	xmlns:gap		= "http://phonegap.com/ns/1.0"
	id			= "rambo"
	android-packageName    	= "com.example.rambo"
	ios-CFBundleIdentifier 	= "com.example.bloopla"
	version 		= "1.0">

	<name>taco taco cli-6.4.0</name>

	<description>
        Try out a minimum app!
	</description>

	<author href="http://adobe.com" email="rudd@adobe.com">
		Brett Rudd
	</author>

  <preference name="phonegap-version" value="cli-6.5.0" />
  <preference name='pgb-builder-version' value='1' />
  <plugin name="cordova-plugin-statusbar" />
  <preference name="StatusBarBackgroundColor" value="#ff0000" />
	<preference name="StatusBarOverlaysWebView" value="true" />
	<preference name="StatusBarStyle" value="blackopaque" />
	<preference name="DisallowOverscroll" value="false" />
	<preference name="DisallowOverscroll" value="true" />
</widget>
